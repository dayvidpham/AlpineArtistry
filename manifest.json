{
  "name": "AlpineArtistry",
  "version": "0.1",
  "description": "Wreak artistic madness onto all the webpages of your liking",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": false,
        "path": "rules/rules-catifier-example.json"
      }
    ]
  },
  "icons": {
    "16": "assets/images/AlpineArtistryLogo16.png",
    "32": "assets/images/AlpineArtistryLogo32.png",
    "48": "assets/images/AlpineArtistryLogo48.png",
    "128": "assets/images/AlpineArtistryLogo128.png"
  },
  "manifest_version": 3
}
